    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "typescript-eslint": "^8.11.0"
  },
  "devDependencies": {
    "@types/eslint-plugin-jsx-a11y": "^6.9.0",
    "@types/eslint__js": "^8.42.3",
    "eslint": "^9.14.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "^9"
