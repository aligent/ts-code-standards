{
  "name": "@aligent/ts-code-standards",
  "version": "1.2.0",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "test": "tsc && eslint .",
    "format": "prettier --ignore-path .prettierignore --check \"**/*.+(js|ts|json)\"",
